{
  "name": "infiniteapi",
  "version": "1.0.0",
  "description": "api for movies and series, for an graphql, mongoose and react training",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./build/index.js --exec babel-node -e js  ",
    "migrate": "nodemon ./build/src/migration/index.js --exec babel-node -e js",
    "build-babel": "babel --source-maps --out-dir ./build ./src",
    "local-start": "nodemon ./src/index.js --exec babel-node -e js",
    "local-migrate": "nodemon ./src/migration/index.js --exec babel-node -e js"
  },
  "author": "Fhillip G. Castillo",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.5",
    "event-stream": "^4.0.1",
    "express": "^4.16.4",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.2",
    "graphql-yoga": "^1.16.7",
    "mongoose": "^5.3.5",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.18.10"
  }
}
